{
  "name": "devloopsnet/laravel-typesense",
  "description": "Typesense laravel/scout engine",
  "keywords": [
    "laravel",
    "typesense",
    "search"
  ],
  "type": "library",
  "homepage": "https://www.devloops.net",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdullah Al-Faqeir",
      "email": "abdullah@devloops.net",
      "homepage": "https://www.devloops.net",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Devloops\\LaravelTypesense\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Devloops\\LaravelTypesense\\TypesenseServiceProvider"
      ],
      "aliases": {
        "Typesense": "Devloops\\LaravelTypesense\\TypesenseFacade"
      }
    }
  },
  "require": {
    "php": ">=7.4",
    "laravel/scout": "^9.1",
    "typesense/typesense-php": "^4.0"
  },
  "suggest": {
    "typesense/typesense-php": "Required to use the Typesense php client."
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "mockery/mockery": "^1.3"
  }
}
